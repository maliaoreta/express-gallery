extends layout

block content
  div#container
    div#staticPhotoDiv
      img#staticPhoto(src="#{staticPhoto}")
    div#photoCollection
      each photo in photoArr
        div.imageContainer
          a(href="http://localhost:3000/gallery/#{photo.id}")
            img(src="#{photo.link}")
            p #{photo.description} - by #{photo.author}
            p #{photo.link}
            div.borderLine