extends layout

block content
  div#container
    div#staticPhotoDiv
      img#staticPhoto(src="#{staticPhoto}")
    div#photoCollection
      each photo in photoArr
        div.aContainer
          a(href="http://localhost:3000/gallery/#{photo.id}")
            div(style="background-image: url(#{photo.link})")#imageContainer
            p #{photo.description} - by #{photo.author}
            p #{photo.link}
        div.borderLine